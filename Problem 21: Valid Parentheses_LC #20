class Solution:
    def isValid(self, s: str) -> bool:
        stack = []
        close_to_open = {
            ')': '(',
            ']':'[',
            '}':'{'           #always have brackets face away from one another
        }

        for char in s:
            if char in close_to_open:
                if not stack or stack[-1] != close_to_open[char]:
                    return False
                stack.pop()
            else:
                stack.append(char)
        return len(stack) == 0
