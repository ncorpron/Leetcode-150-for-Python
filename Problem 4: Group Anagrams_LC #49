class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        anagrams = defaultdict(list)

        for word in strs:
            count = [0]*26  #count 26 lowercase letters
            
            for ch in word:
                count[ord(ch) - ord('a')] += 1    

            anagrams[tuple(count)].append(word)
        return list(anagrams.values())
