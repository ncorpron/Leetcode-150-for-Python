class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:
        left, right = 0, len(numbers) -1

        while left < right:
            curr_sum = numbers[left] + numbers[right] #first number and last number = current sum

            if curr_sum == target:
                return [left + 1, right + 1]  #1-based index, not zero based. This gives two numbers that sum to result
            elif curr_sum < target:
                left += 1     #need a smaller sum ->move left
            else:
                right -= 1  #need a smaller sum -> move right
